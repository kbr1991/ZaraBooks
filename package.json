{
  "name": "zara-books",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development node --env-file=.env --import tsx server/src/index.ts",
    "build": "vite build",
    "start": "npm run build && npm run db:migrate && NODE_ENV=production tsx server/src/index.ts",
    "db:migrate": "tsx server/src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx server/src/db/seed.ts",
    "db:setup": "tsx scripts/setup-db.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "TEST_DATABASE_URL=postgresql://postgres:password@localhost:5433/zarabooks_test vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:ci": "npm run docker:up && sleep 5 && npm run test:run && npm run docker:down"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
